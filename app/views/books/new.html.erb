<h1>Books#new</h1>

<h1>New book</h1>
<!-- form_with部分 -->
<%= form_with model: @book, url: '/books', method: :post do |f| %>

  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>body</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Create book' %>
<% end %>

<p>Find me in app/views/books/new.html.erb</p>
